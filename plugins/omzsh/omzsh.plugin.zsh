# Open editor for your .zshrc
alias zshconfig="code ~/.zshrc"
# Reload all plugins to pickup new changes in current terminal
alias zshreload="source $HOME/.zshrc"
# Change to config directory
alias cdzshconfig='cd ~/.oh-my-zsh/custom'
# Open editor for omzsh.plugin.zsh
alias omzshconfig='code ~/.oh-my-zsh/custom/plugins/omzsh/omzsh.plugin.zsh'